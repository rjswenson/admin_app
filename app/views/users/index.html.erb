<% provide(:title, 'Manifest') %>
<h1> Welcome to the Manifest </h1>
<h2> Below is a list of all users </h2>


<table class="users">
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Email Address</th>
  </tr>
  <% @users.each do |user| %>
    <tr>
      <td> <%= user.first_name %> </td>
      <td> <%= user.last_name %> </td>
      <td> <%= user.email %> </td>
      <td> <%= link_to "[edit]" , '/user.id/edit' %> </td>
      <td> <%= link_to "[delete]" , user, method: :delete, data: {confirm: "You sure?"} %> </td>
    </tr>
  <% end %>
</table>


  
